        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def check_adult(self):\n",
        "        if self.age >= 20:\n",
        "            print(f\"{self.name} is an adult.\")\n",
        "        else:\n",
        "            print(f\"{self.name} is not an adult.\")"
